{
  extends: [
    "eslint-config-airbnb"
  ],
  rules: {
    no-undef: 0,
    no-new: 0,
    no-underscore-dangle: 0,
    arrow-body-style: 0,
    indent: ["error", 4],
    max-len: [2, 160, 2, { "ignoreUrls": true, "ignoreComments": true }],
    comma-dangle: [2, {
        arrays: "always-multiline",
        objects: "always-multiline",
        imports: "always-multiline",
        exports: "always-multiline",
        functions: "ignore",
    }],
    no-else-return: 0,
    import/no-dynamic-require: 0,
    no-return-assign: 0,
    func-names: 0,
    global-require: 0,
    no-param-reassign: 0,
    new-cap: 0,
    class-methods-use-this: 0,
    no-throw-literal: 0,
    no-restricted-syntax: 0,
    no-useless-escape: 0
  }
}
